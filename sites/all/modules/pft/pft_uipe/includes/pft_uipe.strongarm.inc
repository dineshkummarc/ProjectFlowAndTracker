/**
 * Implementation of hook_strongarm()
 */
function pft_uipe_strongarm() {
  $strongarms = array();

  $strongarm = new stdClass;
  $strongarm->disabled = FALSE; /* Edit this to true to make a default strongarm disabled initially */
  $strongarm->api_version = 1;
  $strongarm->name = 'ld_condition_snippet';
  $strongarm->value = '';
  $strongarms['ld_condition_snippet'] = $strongarm;

  $strongarm = new stdClass;
  $strongarm->disabled = FALSE; /* Edit this to true to make a default strongarm disabled initially */
  $strongarm->api_version = 1;
  $strongarm->name = 'ld_condition_type';
  $strongarm->value = 'always';
  $strongarms['ld_condition_type'] = $strongarm;

  $strongarm = new stdClass;
  $strongarm->disabled = FALSE; /* Edit this to true to make a default strongarm disabled initially */
  $strongarm->api_version = 1;
  $strongarm->name = 'ld_destination';
  $strongarm->value = 0;
  $strongarms['ld_destination'] = $strongarm;

  $strongarm = new stdClass;
  $strongarm->disabled = FALSE; /* Edit this to true to make a default strongarm disabled initially */
  $strongarm->api_version = 1;
  $strongarm->name = 'ld_url_destination';
  $strongarm->value = 'user';
  $strongarms['ld_url_destination'] = $strongarm;

  $strongarm = new stdClass;
  $strongarm->disabled = FALSE; /* Edit this to true to make a default strongarm disabled initially */
  $strongarm->api_version = 1;
  $strongarm->name = 'ld_url_type';
  $strongarm->value = 'static';
  $strongarms['ld_url_type'] = $strongarm;

  return $strongarms;
}
